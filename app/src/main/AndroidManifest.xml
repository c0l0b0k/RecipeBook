<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application


        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/recipe_book_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecipeBook"
        tools:targetApi="31">
        <activity
            android:name=".downloaded_recipe"
            android:exported="false"
            android:label="@string/title_activity_downloaded_recipe"
            android:theme="@style/Theme.RecipeBook" />
        <activity
            android:name=".Categories"
            android:exported="false"
            android:label="@string/title_activity_categories"
            android:theme="@style/Theme.RecipeBook" />
        <activity
            android:name=".mealRecipe"
            android:exported="false"
            android:label="@string/title_activity_meal_recipe"
            android:theme="@style/Theme.RecipeBook" />
        <activity
            android:name=".mealRecipte"
            android:exported="false"
            android:label="@string/title_activity_meal_recipte"
            android:theme="@style/Theme.RecipeBook" />
        <activity
            android:name=".RecipesOneGategoria"
            android:exported="false"
            android:label="@string/title_activity_recipes_one_gategoria"
            android:theme="@style/Theme.RecipeBook" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RecipeBook">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>